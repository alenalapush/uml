{
    "cSpell.enabledFileTypes": {
        "*": true,
        "markdown": true
    },
    "cSpell.language": "en, ru",
    "editor.codeActionsOnSave": {
        "source.fixAll.eslint": "explicit"
    },
    "editor.defaultFormatter": "esbenp.prettier-vscode",
    "editor.formatOnSave": true,
    "editor.minimap.enabled": false,
    "editor.suggest.preview": true,
    "editor.wordWrap": "on",
    "explorer.compactFolders": false,
    "explorer.confirmDelete": true,
    "explorer.confirmDragAndDrop": true,
    "terminal.integrated.defaultProfile.windows": "Git Bash",
    "workbench.colorCustomizations": {
        "gitlens.trailingLineForegroundColor": "#888",
        "tab.activeBackground": "#fff3"
    },
    "workbench.tree.indent": 20,
    "[plantuml]": {
        "editor.formatOnSave": true,
        "editor.defaultFormatter": "jebbs.plantuml"
    },
    "plantuml.java": "C:\\Program Files\\Java\\jdk-13.0.1\\bin\\java.exe",
    "plantuml.render": "Local",
    "plantuml.graphvizDot": "C:\\Program Files\\Graphviz\\bin\\dot.exe",
    "plantuml.exportOutDir": "out"
}
